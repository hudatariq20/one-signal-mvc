                <td>
                    @Html.DisplayFor(modelItem => item.safari_site_origin)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.safari_push_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.safari_icon_16_16)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.safari_icon_32_32)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.safari_icon_64_64)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.safari_icon_128_128)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.safari_icon_256_256)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.site_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.created_at)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.updated_at)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.players)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.messageable_players)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.basic_auth_key)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.additional_data_is_root_payload)
                </td>

                <td>
                    
                    @if (User.IsInRole("Admin"))
                    {
                        @Html.ActionLink("Edit", "Update", new { id = item.id });
                        @Html.Raw(" | ");
                    }

                    @Html.ActionLink("Details", "Details", new { id=item.id })
                </td>

            </tr>

            }
</tbody>
    </table>
