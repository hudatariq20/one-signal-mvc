@model IEnumerable<OneSignalAppManager.Models.OneSignalModel>

@{
    ViewData["Title"] = "App - Dashboard";
    
}

<h2>Dashboard</h2>

@if (User.IsInRole("Admin"))
{

    <p>
        <a asp-action="Create">Create New App</a>
    </p>
}
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.gcm_key)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.chrome_key)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.chrome_web_key)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.chrome_web_origin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.chrome_web_gcm_sender_id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.chrome_web_default_notification_icon)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.chrome_web_sub_domain)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.apns_env)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.apns_certificates)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_apns_certificate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_site_origin)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_push_id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_icon_16_16)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_icon_32_32)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_icon_64_64)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_icon_128_128)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.safari_icon_256_256)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.site_name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.created_at)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.updated_at)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.players)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.messageable_players)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.basic_auth_key)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.additional_data_is_root_payload)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
       
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.gcm_key)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.chrome_key)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.chrome_web_key)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.chrome_web_origin)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.chrome_web_gcm_sender_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.chrome_web_default_notification_icon)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.chrome_web_sub_domain)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.apns_env)
